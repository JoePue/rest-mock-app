[
  {
    "name": "Login-Matcher",
    "description": "Define a mocked response for login requests",
    "request": {
      "method": "POST",
      "urlRegEx": "/v1/login.*",
      "bodyRegEx": ".*",
      "bodyContains": "username"
    },
    "response": {
      "statusCode": 200,
      "filename": "/api/login.json"
    }
  },
